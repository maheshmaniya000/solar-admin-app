exports[`TextMarkerComponent component should render component with text & caret 1`] = `
<sg-text-marker
  __ngContext__={[Function LRootView]}
  caretHidden="false"
  elementRef={[Function ElementRef]}
  selected="false"
  selectedChange={[Function EventEmitter_]}
  text={[Function String]}
>
  some text
  <svg
    height="7"
    viewBox="0 0 14 7"
    width="14"
    xmlns="http://www.w3.org/2000/svg"
    xmlns:xlink="http://www.w3.org/1999/xlink"
  >
    <defs>
      <style>
         .b { fill: url(#a); } 
      </style>
      <radialgradient
        cx="0.5"
        cy="0.5"
        gradientUnits="objectBoundingBox"
        id="a"
        r="0.5"
      >
        <stop
          offset="0"
          stop-opacity="0.396"
        />
        <stop
          offset="1"
          stop-opacity="0"
        />
      </radialgradient>
    </defs>
    <ellipse
      class="b"
      cx="7"
      cy="3.5"
      rx="7"
      ry="3.5"
    />
  </svg>
</sg-text-marker>
`;

exports[`TextMarkerComponent component should render component with text & without caret 1`] = `
<sg-text-marker
  __ngContext__={[Function LRootView]}
  caretHidden={[Function Boolean]}
  elementRef={[Function ElementRef]}
  selected="false"
  selectedChange={[Function EventEmitter_]}
  text={[Function String]}
>
  some text
</sg-text-marker>
`;
