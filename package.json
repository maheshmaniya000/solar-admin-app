{
  "name": "solargis2-apps",
  "version": "1.8.9",
  "private": true,
  "description": "Solargis 2 interactive apps",
  "main": "dist/apps/server/index.js",
  "license": "ISC",
  "scripts": {
    "nx": "nx",
    "nx-more-memory": "node --max_old_space_size=2560 ./node_modules/@nrwl/cli/bin/nx",
    "nx:serve": "nx serve",
    "nx:serve:https": "npm run nx:serve -- --ssl true --ssl-key ssl/localhost.key --ssl-cert ssl/localhost.crt",
    "lint": "nx run-many --target=lint --all --quiet",
    "test": "nx run-many --target=test --all",
    "build": "bash build.sh",
    "build:all": "nx run-many --target=build --all --exclude storybook",
    "apps:build": "nx run-many --target=build --projects=admin,dashboard,prospect,print,configurator,evaluate --parallel",
    "libs:build": "nx run-many --target=build --projects=ng-analytics,ng-geosearch,ng-translation,ng-unit-value --parallel",
    "apps:dev": "nx run-many --target=serve --projects=admin,dashboard,prospect,print --parallel --maxParallel=4",
    "build:pipeline:solargis2-apps": "npm run lint && npm run test && nx run-many --target=build --all --exclude storybook components:storybook shared:storybook",
    "build:pipeline:storybook": "nx build storybook && npm run storybook:build",
    "build:precommit": "npm run lint && npm run test && nx run-many --target=build --all --exclude storybook",
    "heroku-prebuild": "bash heroku-prebuild.sh",
    "dev": "nx run-many --target=serve --projects=server,admin,dashboard,prospect,print --parallel --maxParallel=5",
    "dev:admin": "nx run-many --target=serve --projects=server,admin --parallel",
    "dev:dashboard": "nx run-many --target=serve --projects=server,dashboard --parallel",
    "dev:prospect": "nx run-many --target=serve --projects=server,prospect,print --parallel",
    "postinstall": "node tools/postinstall/decorate-angular-cli.js && ngcc --properties es2015 browser module main && node tools/postinstall/post-install-spectator-fix.js",
    "postversion": "npm run libs:build",
    "publish": "git push && npm run libs:build && lerna publish",
    "start": "npm run dev",
    "server:dev": "nx serve server",
    "server:prod": "ENV_PROPERTIES=.env PORT=5000 node dist/apps/server/main.js",
    "server:prod:https": "ENV_PROPERTIES=.env PORT=5000 HTTPS_PORT=5443 node dist/apps/server/main.js",
    "storybook:build": "nx run-many --target=build-storybook --projects=components,shared",
    "storybook:prod": "PORT=4400 node dist/apps/storybook/main.js",
    "lighthouse": "lighthouse --perf --output=pretty http://localhost:5000/prospect",
    "tools:lang:export": "ts-node src/tools/lang-export.ts",
    "tools:lang:check": "ts-node src/tools/lang-check.ts",
    "tools:lang:import": "ts-node src/tools/lang-import.ts",
    "tools:mega-button-assets": "nx workspace-generator mega-button-assets"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run build:precommit"
    }
  },
